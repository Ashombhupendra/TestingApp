<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LikeView">
        <attr name="like_count" format="integer" />
        <attr name="color" format="color" />
        <attr name="liked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MyEditText">
        <attr name="text" format="string" />
        <attr name="placeholder_text" format="string" />
        <attr name="size" format="enum">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
        </attr>
        <attr name="editable" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="editIconTint" format="color" />
        <attr name="hintTextColor" format="color" />
        <attr name="alignToStart" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DetailView">
        <attr name="heading" format="string" />
        <attr name="details" format="string" />
    </declare-styleable>
    <declare-styleable name="ReadMoreTextView">
        <attr name="mainText" format="string" />
    </declare-styleable>
</resources>