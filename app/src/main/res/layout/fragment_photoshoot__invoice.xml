<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="viewmodel"
            type="com.dbvertex.job.peresentation.photoshoot.invoice.InvoiceViewmodel" />
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".peresentation.photoshoot.invoice.Photoshoot_Invoice">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/invoice_date_label"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Date : "
                android:textColor="@color/black"
                android:textSize="20sp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="15dp"
                android:textAlignment="center"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/invoice_date_select"
                />

            <TextView
                android:id="@+id/invoice_date_select"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@={viewmodel.createdDate}"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:textAlignment="center"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/invoice_date_label"
                app:layout_constraintEnd_toEndOf="parent"
                />

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:id="@+id/invoice_bill_to"
                   app:layout_constraintTop_toBottomOf="@id/invoice_date_label"
                   android:layout_width="match_parent"
                   android:layout_marginVertical="20dp"
                   android:background="@drawable/bg_otp_box"
                   android:backgroundTint="@color/white"
                   android:elevation="5dp"
                   android:visibility="visible"
                   android:layout_marginHorizontal="15dp"
                   android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/invoice_bill_to_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:text="Bill To"
                        android:textSize="20sp"
                        android:layout_marginVertical="10dp"
                        android:textColor="@color/black"/>
                     <EditText
                         android:id="@+id/invoice_bill_to_name"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:layout_marginHorizontal="10dp"
                         android:hint="Name"
                         android:text="@={viewmodel.bill_to_name}"
                         android:singleLine="true"
                         android:paddingHorizontal="10dp"
                         android:textColor="@color/black"
                         android:textSize="18sp"
                         android:backgroundTint="#CCC8C8"
                         android:layout_marginVertical="5dp"
                         android:textColorHint="@color/grey"
                         app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_label"
                         />
                   <EditText
                       android:id="@+id/invoice_bill_to_email"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="Email"
                       android:text="@={viewmodel.bill_to_email}"
                       android:maxLength="50"
                       android:singleLine="true"
                       android:paddingHorizontal="10dp"
                       android:layout_marginVertical="5dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_name"
                       />
                   <EditText
                       android:id="@+id/invoice_bill_to_phone"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="Phone Number"
                       android:maxLength="10"
                       android:inputType="number"
                       android:text="@={viewmodel.bill_to_phone}"
                       android:singleLine="true"
                       android:layout_marginVertical="5dp"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_email"
                   />
                   <EditText
                       android:id="@+id/invoice_bill_to_gst_number"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="GST Number"
                       android:text="@={viewmodel.bill_to_gstnumber}"
                       android:singleLine="true"
                        android:layout_marginVertical="5dp"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_phone"
                       />
                   <EditText
                       android:id="@+id/invoice_bill_to_address"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="Address"
                       android:text="@={viewmodel.bill_to_address}"
                       android:singleLine="true"
                       android:layout_marginVertical="5dp"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_gst_number"
                       />
                   <EditText
                       android:id="@+id/invoice_bill_to_city"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="City"
                       android:text="@={viewmodel.bill_to_city}"
                       android:singleLine="true"
                       android:layout_marginVertical="5dp"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_address"
                       />
                   <EditText
                       android:id="@+id/invoice_bill_to_state"
                       android:layout_width="0dp"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="State"
                       android:text="@={viewmodel.bill_to_state}"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:textColorHint="@color/grey"
                       android:layout_marginTop="5dp"
                       android:singleLine="true"
                       android:backgroundTint="#CCC8C8"
                       android:layout_marginBottom="20dp"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintEnd_toStartOf="@id/invoice_bill_to_pincode"
                       app:layout_constraintTop_toBottomOf="@id/invoice_bill_to_city"
                       />
                   <EditText
                       android:id="@+id/invoice_bill_to_pincode"
                       android:layout_width="0dp"
                       android:layout_height="wrap_content"
                       android:layout_marginHorizontal="10dp"
                       android:hint="Pincode"
                       android:text="@={viewmodel.bill_to_pincode}"
                       android:singleLine="true"
                       android:inputType="number"
                       android:maxLength="6"
                       android:layout_marginVertical="5dp"
                       android:paddingHorizontal="10dp"
                       android:textColor="@color/text_color"
                       android:textSize="18sp"
                       android:backgroundTint="#CCC8C8"
                       android:textColorHint="@color/grey"
                       app:layout_constraintTop_toTopOf="@id/invoice_bill_to_state"
                       app:layout_constraintBottom_toBottomOf="@id/invoice_bill_to_state"
                       app:layout_constraintStart_toEndOf="@id/invoice_bill_to_state"
                       app:layout_constraintEnd_toEndOf="parent"
                       />

               </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/invoice_bill_from"
                app:layout_constraintTop_toBottomOf="@id/invoice_bill_to"
                android:layout_width="match_parent"
                android:layout_marginVertical="20dp"
                android:background="@drawable/bg_otp_box"
                android:backgroundTint="@color/white"
                android:elevation="5dp"
                android:visibility="visible"
                android:layout_marginHorizontal="15dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/invoice_bill_from_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Bill From"
                    android:textSize="20sp"
                    android:layout_marginVertical="10dp"
                    android:textColor="@color/black"/>
                <EditText
                    android:id="@+id/invoice_bill_from_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Name"
                    android:text="@={viewmodel.bill_from_name}"
                    android:singleLine="true"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:layout_marginVertical="5dp"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_label"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Email"
                    android:text="@={viewmodel.bill_from_email}"
                    android:maxLength="30"
                    android:singleLine="true"
                    android:paddingHorizontal="10dp"
                    android:layout_marginVertical="5dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_name"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Phone Number"
                    android:maxLength="10"
                    android:inputType="number"
                    android:text="@={viewmodel.bill_from_phone}"
                    android:singleLine="true"
                    android:backgroundTint="#CCC8C8"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_email"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_gst_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="GST Number"
                    android:text="@={viewmodel.bill_from_gstnumber}"
                    android:singleLine="true"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_phone"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Address"
                    android:text="@={viewmodel.bill_from_address}"
                    android:singleLine="true"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_gst_number"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="City"
                    android:text="@={viewmodel.bill_from_city}"
                    android:backgroundTint="#CCC8C8"
                    android:singleLine="true"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_address"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_state"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="State"
                    android:text="@={viewmodel.bill_from_state}"
                    android:backgroundTint="#CCC8C8"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:textColorHint="@color/grey"
                    android:layout_marginTop="5dp"
                    android:singleLine="true"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/invoice_bill_from_pincode"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_from_city"
                    />
                <EditText
                    android:id="@+id/invoice_bill_from_pincode"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Pincode"
                    android:inputType="number"
                    android:text="@={viewmodel.bill_from_pincode}"
                    android:backgroundTint="#CCC8C8"
                    android:singleLine="true"
                    android:maxLength="6"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toTopOf="@id/invoice_bill_from_state"
                    app:layout_constraintBottom_toBottomOf="@id/invoice_bill_from_state"
                    app:layout_constraintStart_toEndOf="@id/invoice_bill_from_state"
                    app:layout_constraintEnd_toEndOf="parent"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/invoice_bill_item"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/invoice_bill_from"
                android:layout_marginVertical="20dp"
                android:background="@drawable/bg_otp_box"
                android:backgroundTint="@color/white"
                android:elevation="5dp"
                android:visibility="visible"
                android:layout_marginHorizontal="15dp"
                >

                <TextView
                    android:id="@+id/invoice_bill_item_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Item"
                    android:textSize="20sp"
                    android:layout_marginVertical="10dp"
                    android:textColor="@color/black"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/invoice_bill_item_rv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_item_label"
                    android:layout_marginHorizontal="5dp"
                    android:layout_marginVertical="10dp"/>

                <LinearLayout
                    android:id="@+id/item_photoshoot_presaved_add_new_msg"
                    android:layout_width="match_parent"
                    android:layout_marginVertical="10dp"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_item_rv"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginHorizontal="15dp"
                    android:padding="5dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:background="@drawable/cancel_new"
                        android:backgroundTint="@color/blue_dark"
                        android:src="@drawable/ic_baseline_add_24"
                        app:tint="@color/white" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Add Item"
                        android:textColor="@color/blue_dark"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:layout_marginHorizontal="10dp"/>

                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/invoice_bill_bank_detail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/invoice_bill_item"
                android:layout_marginVertical="20dp"
                android:background="@drawable/bg_otp_box"
                android:backgroundTint="@color/white"
                android:elevation="5dp"

                android:layout_marginHorizontal="15dp"
                >

                <TextView
                    android:id="@+id/invoice_bill_bank_detail_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Bank Detail"
                    android:textSize="20sp"
                    android:layout_marginVertical="10dp"
                    android:textColor="@color/black"/>

                <EditText
                    android:id="@+id/invoice_bill_bank_detail_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Name"
                    android:text="@={viewmodel.account_holder_name}"
                    android:singleLine="true"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:layout_marginVertical="5dp"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail_label"
                    />
                <EditText
                    android:id="@+id/invoice_bill_bank_detail_bank_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Bank Name"
                    android:text="@={viewmodel.bank_name}"
                    android:singleLine="true"
                    android:paddingHorizontal="10dp"
                    android:layout_marginVertical="5dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail_name"
                    />
                <EditText
                    android:id="@+id/invoice_bill_bank_detail_bank_acccount_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Account Number"
                    android:singleLine="true"
                    android:text="@={viewmodel.account_number}"
                    android:inputType="number"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail_bank_name"
                    />
                <EditText
                    android:id="@+id/invoice_bill_bank_detail_ifsc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="IFSC Code"
                    android:text="@={viewmodel.bank_ifsc}"
                    android:singleLine="true"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail_bank_acccount_num"
                    />
                <EditText
                    android:id="@+id/invoice_bill_account_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="10dp"
                    android:hint="Account Type"
                    android:text="@={viewmodel.account_type}"
                    android:singleLine="true"
                    android:layout_marginVertical="5dp"
                    android:paddingHorizontal="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="18sp"
                    android:backgroundTint="#CCC8C8"
                    android:textColorHint="@color/grey"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail_ifsc"
                    />



            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/invoice_bill_bank_detail"
                android:background="@color/transparent"
                android:layout_marginVertical="20dp"
                android:layout_height="wrap_content">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginBottom="20dp"
                    android:background="@color/transparent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/photoshoot_preview_invoice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/photoshoot_generate_invoice"
                        android:text="Preview"
                        android:paddingHorizontal="40dp"
                        android:paddingVertical="10dp"
                        android:textSize="15sp"
                        android:textAlignment="center"
                        android:elevation="10dp"
                        android:textStyle="bold"
                        android:backgroundTint="@color/blue_gray"
                       android:layout_marginStart="20dp"
                        android:background="@drawable/bg_free_item"
                        android:textColor="@color/blue_dark"
                        />
                    <TextView
                        android:id="@+id/photoshoot_generate_invoice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/photoshoot_preview_invoice"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Generate Invoice"
                        android:layout_marginEnd="20dp"
                        android:textSize="15sp"
                        android:elevation="10dp"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:background="@drawable/bg_free_item"
                        android:backgroundTint="@color/blue_dark"
                        android:textColor="@color/white"
                        android:padding="10dp"
                        />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <ImageView
                android:id="@+id/category_loading"
                android:layout_width="40dp"
                android:layout_height="44dp"
                android:visibility="gone"
                 app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_gravity="center_horizontal"
                android:background="@android:color/transparent"
                android:backgroundTint="@android:color/transparent"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>
</layout>